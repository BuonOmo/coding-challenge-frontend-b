{
  "name": "coding-challenge-frontend-b",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest-cli": "^22.1.3",
    "react-scripts": "1.1.0",
    "xo": "^0.18.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "xo && jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|svg)$": "<rootDir>/jestStub.js"
    }
  },
  "xo": {
    "plugin": "react",
    "extend": "react-app",
    "envs": [
      "browser",
      "jest",
      "worker"
    ],
    "rules": {
      "import/no-unassigned-import": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "semicolon": false
  }
}
